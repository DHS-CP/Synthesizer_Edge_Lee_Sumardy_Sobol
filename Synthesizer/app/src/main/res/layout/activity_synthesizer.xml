<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.lee55140.synthesizer.SynthesizerActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/fButton"
        android:layout_marginEnd="76dp"
        android:layout_marginTop="67dp"
        android:text="Hello World!"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:id="@+id/textView" />

    <Button
        android:id="@+id/eButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onButton1Click"
        android:text="@string/button1"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <Button
        android:id="@+id/fButton"
        android:layout_width="wrap_content"
        android:layout_height="49dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/eButton"
        android:layout_marginTop="19dp"
        android:onClick="onButton2Click"
        android:text="@string/button2"
        tools:layout_editor_absoluteX="0dp" />

    <Button
        android:id="@+id/aButton"
        android:layout_width="wrap_content"
        android:layout_height="49dp"
        android:layout_above="@+id/fButton"
        android:layout_marginStart="14dp"
        android:layout_toEndOf="@+id/eButton"
        android:onClick="onButton3Click"
        android:text="@string/button3"
        tools:layout_editor_absoluteX="0dp" />

    <Button
        android:id="@+id/bButton"
        android:layout_width="wrap_content"
        android:layout_height="49dp"
        android:layout_alignBaseline="@+id/fButton"
        android:layout_alignBottom="@+id/fButton"
        android:layout_toEndOf="@+id/fButton"
        android:onClick="onButton4Click"
        android:text="@string/button4"
        tools:layout_editor_absoluteX="0dp" />

    <Button
        android:id="@+id/bbButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/fButton"
        android:layout_alignEnd="@+id/textView"
        android:onClick="onButton5Click"
        android:layout_marginEnd="13dp"
        android:text="@string/button5" />

    <Button
        android:id="@+id/cButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/fButton"
        android:layout_toEndOf="@+id/aButton"
        android:onClick="onButton6Click"
        android:text="@string/button6" />

    <Button
        android:id="@+id/csButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/cButton"
        android:layout_alignBottom="@+id/cButton"
        android:layout_alignParentEnd="true"
        android:onClick="onButton7Click"
        android:text="@string/button7" />

    <Button
        android:id="@+id/dButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/bButton"
        android:layout_marginTop="14dp"
        android:onClick="onButton8Click"
        android:text="@string/button8" />

    <Button
        android:id="@+id/dsButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/dButton"
        android:layout_alignBottom="@+id/dButton"
        android:layout_toStartOf="@+id/cButton"
        android:onClick="onButton9Click"
        android:text="@string/button9" />

    <Button
        android:id="@+id/fsButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/dsButton"
        android:layout_alignBottom="@+id/dsButton"
        android:layout_toEndOf="@+id/dsButton"
        android:onClick="onButton10Click"
        android:text="@string/button10" />

    <Button
        android:id="@+id/gButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/fsButton"
        android:layout_alignBottom="@+id/fsButton"
        android:layout_toEndOf="@+id/fsButton"
        android:onClick="onButton11Click"
        android:text="@string/button11" />

    <Button
        android:id="@+id/gsButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/textView"
        android:onClick="onButton12Click"
        android:text="@string/button12" />

    <Button
        android:id="@+id/higheButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/gsButton"
        android:layout_alignBottom="@+id/gsButton"
        android:layout_alignStart="@+id/aButton"
        android:onClick="onButton13Click"
        android:text="@string/button13" />

    <Button
        android:id="@+id/highfButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView"
        android:layout_alignTop="@+id/textView"
        android:onClick="onButton14Click"
        android:text="@string/button14" />

    <Button
        android:id="@+id/highfsButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:onClick="onButton15Click"
        android:text="@string/button15" />

    <Button
        android:id="@+id/highgButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/highfsButton"
        android:layout_toEndOf="@+id/highfsButton"
        android:onClick="onButton16Click"
        android:text="@string/button16" />

    <Button
        android:id="@+id/twinkle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/highgButton"
        android:layout_below="@+id/highfsButton"
        android:layout_marginTop="82dp"
        android:text="@string/button17" />
    
        <Button
        android:id="@+id/Sequence"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/highgButton"
        android:layout_below="@+id/highfsButton"
        android:layout_marginTop="82dp"
        android:text="@string/button19" />


</RelativeLayout>
